model Comment {
  id        String       @id @default(uuid())
  createdAt DateTime     @default(now())
  content   String
  Tweet     Tweet        @relation(fields: [tweetId], references: [id], onDelete: Cascade)
  tweetId   String
  User      User         @relation(fields: [userId], references: [id], onDelete: Cascade)
  userId    String
}